{
	"signature": "gravelbox_config.sig",
	"password": "",
	"syscall-definition": "syscalldef.json",
	"pinentry": "pinentry",
	"max-string-length": 128,
	"default-action": "allow",
	"action-groups": [
		{
			"action": "ask",
			"patterns": [
				"syscall32(.*)"
			]
		},
		{
			"action": "ask",
			"patterns": [
				"openat\\(.*, <fault>, \\d+, \\d+\\)"
			]
		},
		{
			"action": "ask",
			"patterns": [
				"openat\\(-?\\d+, \"[^/.].*\"(\\.\\.\\.)?, \\d+, \\d+\\)",
				"open\\(\"[^/.].*\"(\\.\\.\\.)?, \\d+, \\d+\\)",
				"openat\\(-?\\d+, \"/home/\\w+/[^.].*\"(\\.\\.\\.)?, \\d+, \\d+\\)",
				"open\\(\"/home/\\w+/[^.].*\"(\\.\\.\\.)?, \\d+, \\d+\\)"
			]
		}
	]
}
