{
	"syscall-definition": "syscalldef.json",
	"pinentry": "pinentry",
	"max-string-length": 64,
	"default-action": "allow",
	"action-groups": [
		{
			"action": "ask",
			"patterns": [
				"openat\\(-?\\d+, \"/home/[a-zA-Z]+/[^.].*\"(\\.\\.\\.)?, \\d+, \\d+\\)",
				"open\\(\"/home/[a-zA-Z]+/[^.].*\"(\\.\\.\\.)?, \\d+, \\d+\\)"
			]
		},
		{
			"action": "deny",
			"patterns": [
				"socket\\(.*\\)"
			]
		}
	]
}
